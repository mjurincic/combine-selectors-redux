{
  "name": "combine-selectors",
  "version": "0.0.0",
  "description": "Utility function for combining selectors when working with redux, much like combineReducers it helps for defining selector functions within the reducers.",
  "main": "index.js",
  "repository": "git@gitlab.com:tpayne/combine-selectors.git",
  "author": "Tom Payne <thomas.payne@skybettingandgaming.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.12.0",
    "jest": "^21.2.1"
  },
  "scripts": {
    "build": "babel --out-dir lib src",
    "lint": "eslint -- src/**/*js tests/**/*.js",
    "test": "jest"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "jest": {
    "roots": ["<rootDir>/tests/"],
    "testMatch": ["**/*.js"],
    "testPathIgnorePatterns": ["/__mocks__/"]
  }
}
